/*
10. Escribir un programa que almacene la cadena de caracteres contraseña en una
variable, pregunte al usuario por la contraseña hasta que introduzca la contraseña correcta
*/

#include <iostream>
using namespace std;
int main(int argc, char *argv[]) {
char a[20];
string b="hola";
cout<<"INGRESE LA CONTRASEñA? "<<endl;
cin>>a;
while(a!=b){
cout<<"INGRESE LA CONTRASEñA CORRECTA? "<<endl;
cin>>a;
}
return 0;
}
