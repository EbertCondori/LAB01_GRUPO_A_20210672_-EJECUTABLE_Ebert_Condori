/*
7. Elabore un algoritmo que lea por teclado dos n√∫meros enteros y determine si uno es
divisor del otro.
*/

include <iostream>
using namespace std;
int main(int argc, char *argv[]) {
int a;
int b;
cout<<"ingrese 2 numeros a y b "<<endl;
cin>>a>>b;
if(a%b==0)
cout<<"b es divisor de a"<<endl;
if(b%a==0)
cout<<"a es divisor de b"<<endl;
if(a%b!=0 && b%a!=0)
cout<<"no son divisores"<<endl;
return 0;
}
